<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
		PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
		"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

	<context id="default" targetRuntime="MyBatis3">

		<!-- 格式化java代码 -->
		<property name="javaFormatter" value="org.mybatis.generator.api.dom.DefaultJavaFormatter" />
		<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
		<commentGenerator>
			<property name="suppressAllComments" value="true"/>
			<!--生成的注释包含时间戳-->
			<property name="suppressDate" value="true"/>
		</commentGenerator>

		<!--数据库连接的信息：驱动类、连接地址、用户名、密码 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
						connectionURL="jdbc:mysql://10.231.128.148:3306/lc_acc_db?useUnicode=true&amp;characterEncoding=UTF-8" userId="root"
						password="123456aA!">
			<property name="useInformationSchema" value="true"/>
		</jdbcConnection>

		<!-- 指定生成的类型为java类型，避免数据库中number等类型字段 -->
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<!-- 生成model模型，对应的包，存放位置可以指定具体的路径,如/ProjectName/src，也可以使用MAVEN来自动生成 -->
		<javaModelGenerator targetPackage="com.longfor.gf.lc.account.dao.entity" targetProject="C:\Users\guoguangxiao\Desktop\temp">
			<property name="enableSubPackages" value="true" />
		</javaModelGenerator>

		<!--对应的xml mapper文件 -->
		<sqlMapGenerator targetPackage="com.longfor.gf.lc.account.dao.mapper" targetProject="C:\Users\guoguangxiao\Desktop\temp">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<!-- 对应的dao接口 -->
		<javaClientGenerator type="XMLMAPPER" targetPackage="com.longfor.gf.lc.account.dao" targetProject="C:\Users\guoguangxiao\Desktop\temp">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<!--<table tableName="t_gat_gr_account" domainObjectName="GatGrAccount" modelType="flat"-->
			   <!--enableCountByExample="true" enableUpdateByExample="true"-->
			   <!--enableDeleteByExample="true" enableSelectByExample="true"-->
			   <!--selectByExampleQueryId="true">-->
		<!--</table>-->
		<!--<table tableName="t_yw_account" domainObjectName="YwAccount" modelType="flat"-->
			   <!--enableCountByExample="true" enableUpdateByExample="true"-->
			   <!--enableDeleteByExample="true" enableSelectByExample="true"-->
			   <!--selectByExampleQueryId="true">-->
		<!--</table>-->
		<table tableName="t_transaction" domainObjectName="Transaction" modelType="flat"
		enableCountByExample="true" enableUpdateByExample="true"
		enableDeleteByExample="true" enableSelectByExample="true"
		selectByExampleQueryId="true">
		</table>
	</context>
</generatorConfiguration>
